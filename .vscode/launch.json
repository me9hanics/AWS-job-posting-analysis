    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "${command:python.interpreterPath}"
        },
        {
            "name": "Python: Legal Collect",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/schedule/legal_collect.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "${command:python.interpreterPath}",
            "cwd": "${workspaceFolder}"
        }
    ]
}